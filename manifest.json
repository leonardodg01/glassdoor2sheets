{
  "name": "Glassdoor2Sheets",
  "version": "1.0",
  "description": "Google extension for moving job information from Glassdoor (and potentially other jobboards) to a Google Sheets doc.",
  "manifest_version": 3,
  "action": {
    "default_title": "Glassdoor2Sheets",
    "default_popup": "dist/index.html"
  },
  "background": {
    "service_worker": "./dist/service-worker.js",
    "type": "module"
  },
  "oauth2": {
    "client_id": "531430626938-f93umk335vt69f83g852s5lnfsodnf3q.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/spreadsheets"]
  },
  "permissions": [
    "identity",
    "storage",
    "https://sheets.googleapis.com/*"
  ],
  "key": ""
}